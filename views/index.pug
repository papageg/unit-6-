extends layout.pug

block content 
  article.grid-container.portfolio-intro
    div

      h1 Portfolio

      p.lead.text-light
        | Make the web beautiful one page at a time.
  article.grid-container.portfolio-index
    
    .grid-x.grid-margin-x.small-up-2.medium-up-2.large-up-3

      p
        each project in projects
            .cell
              a(href=`${project.live_link}`, target="_blank")
                img.thumbnail(src=`${project.image_urls[1]}`)
                h5= project.projectName

      
